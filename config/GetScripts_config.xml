<?xml version="1.0"?>
<getthis reportall="">
  <output password="V!ruS" compression="ultra"/>
  <location altitude="highest" shadow="yes">*</location>
  <samples MaxPerSampleBytes="300KB" MaxSampleCount="5000" MaxTotalBytes="150MB">
    <sample name="job">
        <ntfs_find name_match="*.job" />
    </sample>
    <sample name="at">
        <ntfs_find name_match="*.at" />
    </sample>
    <sample name="vbs_vbe">
        <ntfs_find name_match="*.vbs" />
        <ntfs_find name_match="*.vbe" />
    </sample>
    <sample name="bat_cmd">
        <ntfs_find name_match="*.bat" />
        <ntfs_find name_match="*.cmd" />
    </sample>
    <sample name="ps1">
        <ntfs_find name_match="*.ps1" />
    </sample>
    <sample name="py">
        <ntfs_find name_match="*.py" />
    </sample>
    <sample name="wmi">
        <ntfs_find name_math="*.mof" />
    </sample>
    <sample name="jar">
      <ntfs_find path_match="\Users\*.jar" />
      <ntfs_find path_match="\Document and settings\*.jar" />
    </sample>
    <sample name="aspx">
      <ntfs_find name_match="*.aspx" />
      <ntfs_find name_math="*.asp" />
    </sample>
  </samples>
</getthis>
