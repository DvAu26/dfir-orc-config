  <samples MaxPerSampleBytes="50MB" MaxSampleCount="10000" MaxTotalBytes="250MB">
    <sample name="CatRoot">
      <ntfs_find name_match="*.cat" header_hex="30"/>
    </sample>
  </samples>
</getthis>
